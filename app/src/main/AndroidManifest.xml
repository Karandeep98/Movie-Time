<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="codingblocks.com.networkokhttp">

    <uses-permission android:name="android.permission.INTERNET">
    </uses-permission>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="IMDB Clone"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Tvshows.ViewAllTvShows">
        </activity>
        <activity
                android:name=".Tvshows.TvDetails"
                android:parentActivityName=".Tvshows.TVMainActivity">
        </activity>
        <activity android:name=".Tvshows.TVMainActivity">
        </activity>
        <activity android:name=".Reviews">
        </activity>
        <activity android:name=".Youtubeplay">
        </activity>
        <activity
                android:name=".About"
                android:launchMode="singleInstance"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".ViewAll"
                android:launchMode="singleInstance"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".NavigationActivity">
        </activity>
        <activity android:name=".Favourites">
        </activity>
        <activity
                android:name=".Search"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".Room"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".Castdetails"
                android:parentActivityName=".Castdetails">
        </activity>
        <activity
                android:name=".Details"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service
                android:name=".MyMessagingService"
                android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>